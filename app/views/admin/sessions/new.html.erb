<div class="mx-auto" style="padding: 50px">
  <div class="mx-auto" style="width: 650px">
    <nav class="navbar navbar-expand-sm navbar-light bg-light mt-3 mb-3">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav4" aria-controls="navbarNav4" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-end">
            <ul class="navbar-nav">
              <li class="nav-item active"> 
              </li>
            </ul>
          </div>
          <div class="container">
            <p class="text-left">管理者ログイン</p>
          </div>
  </div>
</div>
<%# resource = Admin.new unless resource %>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="mx-auto" style="width: 300px">
  <div class="form-group row my-100">
     
     
     
     <div class="field">
      <%= f.label :email,"メールアドレス" %>
    <p><input type="email" placeholder="メールアドレス" required autofocus></p>
     </div>
    
<p><input type="password" placeholder="パスワード" required></p>
    
    
    <div class="field">
      <%= f.label :email,"メールアドレス" %>
      <%= f.email_field :email, autofocus: true, autocomplete: "email",placeholder: "sampla@example.com" %>
    </div>
    <div class="field">
      <%= f.label :password,"パスワード" %>
      <%= f.password_field :password, autocomplete: "current-password" %>
    </div>
  </div>
  <div class="actions">
    <div class="text-center my-3">
      <%= f.submit "ログイン",class: "btn btn-primary"%>
    </div>  
  </div>
    
  <% end %>
   </nav>

</div>
